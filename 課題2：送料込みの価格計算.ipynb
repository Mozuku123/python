   "source": [
    "price=input(\"税抜価格を入力してください：\")\n",
    "price_with_tax=round(int(price)*1.1)\n",
    "if int(price_with_tax)>=2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料として350円かかります\")\n",
    "    price_with_tax+=350\n",
    "print(f\"送料込みの価格は{str(price_with_tax)}円です。\")"
   ]
  }
