    "sum=0\n",
    "count=0\n",
    "while True:\n",
    "    score=input(\"点数を入力してください：\")\n",
    "    print(f\"点数を入力してください: {score}\")\n",
    "    if int(score)==-1:\n",
    "        break\n",
    "    sum=int(score)+sum\n",
    "    count=count+1\n",
    "\n",
    "print(f\"{count}人のテストの平均点は{round(sum/count,1)}点です\")\n",
    "\n",
    "\n"
   ]
